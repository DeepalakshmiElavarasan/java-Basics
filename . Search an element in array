public class SearchElement {
    public static void main(String[] args) {
        int[] arr = {5, 10, 15, 20};
        int key = 15;
        boolean found = false;

        for (int n : arr) {
            if (n == key) {
                found = true;
                break;
            }
        }

        if (found)
            System.out.println(key + " found");
        else
            System.out.println(key + " not found");
    }
}
