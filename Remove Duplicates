class RemoveDuplicates {
    public static void main(String[] args) {
        int[] arr = {1, 2, 2, 3, 4, 4};
        boolean[] seen = new boolean[101]; 

        for (int num : arr) {
            if (!seen[num]) {
                System.out.print(num + " ");
                seen[num] = true;
            }
        }
    }
}
