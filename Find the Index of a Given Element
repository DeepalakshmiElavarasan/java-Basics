class FindIndex {
    public static void main(String[] args) {
        int[] arr = {7, 5, 9, 1};
        int target = 9;

        int index = -1;
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] == target) index = i;
        }

        System.out.println("Index: " + index);
    }
}

