class FirstRepeating {
    public static void main(String[] args) {
        int[] arr = {3, 5, 2, 5, 1, 3};

        for (int i = 0; i < arr.length; i++) {
            for (int j = i + 1; j < arr.length; j++) {
                if (arr[i] == arr[j]) {
                    System.out.println("First repeating: " + arr[i]);
                    return;
                }
            }
        }
        System.out.println("No repeating elements");
    }
}
