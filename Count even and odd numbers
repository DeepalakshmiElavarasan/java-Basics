public class CountEvenOdd {
    public static void main(String[] args) {
        int[] arr = {10, 21, 33, 40, 55};
        int even = 0, odd = 0;

        for (int n : arr) {
            if (n % 2 == 0)
                even++;
            else
                odd++;
        }

        System.out.println("Even numbers = " + even);
        System.out.println("Odd numbers = " + odd);
    }
}
