class UniqueElements {
    public static void main(String[] args) {
        int[] arr = {4, 3, 2, 4, 1, 2, 5};

        for (int i = 0; i < arr.length; i++) {
            boolean unique = true;

            for (int j = 0; j < arr.length; j++) {
                if (i != j && arr[i] == arr[j]) {
                    unique = false;
                }
            }

            if (unique) System.out.print(arr[i] + " ");
        }
    }
}
