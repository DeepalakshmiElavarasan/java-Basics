public class subArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] a= {2,6,4,8};
		solve(a);
	}
	
	public static void solve(int[]a)
	
	{
		for(int i=0;i<a.length;i++)
		{
			for(int j=i;j<a.length;j++)
				
			{
				for(int k=i;k<=j;k++)
				{
					System.out.print(a[k]+" ");
				}
				System.out.print(" ");
				
			}
			
			System.out.println();
		}
	}
	
}
