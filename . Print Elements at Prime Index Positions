class PrimeIndexElements {
    public static void main(String[] args) {
        int[] arr = {5, 10, 15, 20, 25, 30};

        for (int i = 2; i < arr.length; i++) {
            boolean prime = true;

            for (int j = 2; j <= i / 2; j++) {
                if (i % j == 0) prime = false;
            }

            if (prime) System.out.print(arr[i] + " ");
        }
    }
}
